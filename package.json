{
  "name": "lingua-poly",
  "version": "0.1.0",
  "description": "A system for disassembling languages",
  "homepage": "https://github.com/gflohr/Lingua-Poly",
  "bugs": {
    "url": "https://github.com/gflohr/Lingua-Poly/issues"
  },
  "license": "WTFPL",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config=proxy.conf.json",
    "build": "ng build",
    "postinstall": "ngcc",
    "maildev": "maildev",
    "test": "ng test",
    "lint": "ng lint --exclude=./src/app/core/openapi/**/*.ts",
    "e2e": "ng e2e",
    "xgettext": "node node_modules/@biesbjerg/ngx-translate-extract/bin/cli.js --input=./src --output=src/assets/i18n/en.po --clean --sort --format=pot --marker=_",
    "openapi-generate": "openapi-generator generate -i apis/users/public/openapi.yaml -g typescript-angular -o src/app/core/openapi/lingua-poly"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/animations": "^9.1.2",
    "@angular/common": "^9.1.2",
    "@angular/compiler": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/forms": "^9.1.2",
    "@angular/localize": "^9.1.2",
    "@angular/platform-browser": "^9.1.2",
    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.3",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/router-store": "^9.1.0",
    "@ngrx/store": "^9.1.0",
    "@ngrx/store-devtools": "9.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@openapitools/openapi-generator-cli": "^1.0.2-4.2.0",
    "@types/zxcvbn": "^4.4.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "karma": "^5.0.2",
    "ngx-cookie-service": "^3.0.4",
    "rxjs": "^6.3.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "^9.1.2",
    "@angular/language-service": "^9.1.2",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "maildev": "^1.1.0",
    "protractor": "^5.4.4",
    "ts-node": "~5.0.1",
    "tslint": "~5.20.1",
    "typescript": "~3.8.3"
  }
}