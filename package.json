{
  "name": "lingua-poly",
  "version": "0.1.0",
  "description": "A system for disassembling languages",
  "homepage": "https://github.com/gflohr/Lingua-Poly",
  "bugs": {
    "url": "https://github.com/gflohr/Lingua-Poly/issues"
  },
  "license": "WTFPL",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config=proxy.conf.json",
    "build": "ng build",
    "maildev": "maildev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "node node_modules/@biesbjerg/ngx-translate-extract/bin/cli.js --input=./src --output=src/assets/i18n/en.po --clean --sort --format=pot --marker=_",
    "generate-openapi": "openapi-generator generate -i perl/public/openapi.yaml -g typescript-angular -o src/app/core/openapi/lingua-poly"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.13",
    "@angular/common": "^8.2.13",
    "@angular/compiler": "^8.2.13",
    "@angular/core": "^8.2.13",
    "@angular/forms": "^8.2.13",
    "@angular/platform-browser": "^8.2.13",
    "@angular/platform-browser-dynamic": "^8.2.13",
    "@angular/router": "^8.2.13",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.2",
    "@ngrx/effects": "^8.5.2",
    "@ngrx/router-store": "^8.5.2",
    "@ngrx/store": "^8.5.2",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@openapitools/openapi-generator-cli": "^1.0.2-4.2.0",
    "@types/zxcvbn": "^4.4.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.2",
    "zone.js": "^0.9.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^8.2.13",
    "@angular/language-service": "^8.2.13",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "maildev": "^1.1.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3"
  }
}
