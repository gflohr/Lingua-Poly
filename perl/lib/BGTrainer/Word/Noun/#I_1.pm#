#! /bin/false

package BGTrainer::Word::Noun::I_1;

use strict;

use utf8;

use base qw (BGTrainer::Word::Noun);

sub none { 
    my ($self) = @_;
    
    my $word = $self->{_word};
    my @stresses = @{$self->{_stresses}};

    return (
	    $self->_emphasize ($word, @stresses),
	    $self->_emphasize ($word . 'œô¨Ð', -1),
	    $self->_emphasize ($word . 'œô¨êœô¨â', -1),
	    $self->_emphasize ($word . 'œô¨Þœô¨Òœô¨Õ', -1),
	    $self->_emphasize ($word . 'œô¨Þœô¨Òœô¨Õœô¨âœô¨Õ', -2),
	    $self->_emphasize ($word . 'œô¨Ð', @stresses),
	    undef,
	    );
}

1;

#Local Variables:
#mode: perl
#perl-indent-level: 4
#perl-continued-statement-offset: 4
#perl-continued-brace-offset: 0
#perl-brace-offset: -4
#perl-brace-imaginary-offset: 0
#perl-label-offset: -4
#cperl-indent-level: 4
#cperl-continued-statement-offset: 2
#tab-width: 8
#coding: utf-8
#End:


    my $dim = $word;

    return (
	    $self->_emphasize ('œô¨Ðœô¨Ûœô¨ãœô¨Üœô¨Øœô¨Ýœô¨Øœô¨Ù', @stresses),
	    $self->_emphasize ('œô¨Ðœô¨Ûœô¨ãœô¨Üœô¨Øœô¨Ýœô¨Øœô¨ï', @stresses),
	    $self->_emphasize ('œô¨Ðœô¨Ûœô¨ãœô¨Üœô¨Øœô¨Ýœô¨Øœô¨ïœô¨â', @stresses),
	    $self->_emphasize ('œô¨Ðœô¨Ûœô¨ãœô¨Üœô¨Øœô¨Ýœô¨Øœô¨Ø', @stresses),
	    $self->_emphasize ('œô¨Ðœô¨Ûœô¨ãœô¨Üœô¨Øœô¨Ýœô¨Øœô¨Øœô¨âœô¨Õ', @stresses),
	    $self->_emphasize ('œô¨Ðœô¨Ûœô¨ãœô¨Üœô¨Øœô¨Ýœô¨Øœô¨ï', @stresses),
	    $self->_emphasize (undef, @stresses),
	    );
}

1;

